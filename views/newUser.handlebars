
    <h1>Create An Account!</h1>
    <form id="form_id" class = "newUser-form" method = "POST">
      <p>Please use only letters and numbers in your username</p>
      <label>{{error}}</label>
      <label for="username">Username:</label>
      <input type = "text" name = "username" id = "username"><br>

      <label for="password">Password:</label>
      <input type = "password" name = "password" id = "password"><br>

      <label for="email">E-Mail:</label>
      <input type = "text" name = "email" id = "email"><br>
      

      
      <input type = "submit" value = "submit" id="displayObj"/><br>
      <input type="button" onclick="location.href='/login';" value="Already have a username? Log In" />

    </form>

